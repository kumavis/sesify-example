{
  "name": "browserify-example",
  "version": "1.0.0",
  "description": "example with browserify + npm",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "email": "dave.des@gmail.com",
    "url": "https://github.com/mattdesl"
  },
  "dependencies": {
    "tone": "^0.6.0"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "budo": "^10.0.4",
    "uglify-js": "^3.3.4"
  },
  "scripts": {
    "start": "budo index.js:bundle.js --live -- -p [ sesify --config ./sesify-config.js ]",
    "sesify-config": "browserify index.js -p [ sesify --writeAutoConfig ./sesify-config.js ] | echo 'config autogen complete'",
    "build": "browserify index.js | uglifyjs -m -c warnings=false > bundle.js"
  },
  "keywords": [
    "budo",
    "browserify",
    "example",
    "help",
    "tutorial",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/browserify-example.git"
  },
  "homepage": "https://github.com/mattdesl/browserify-example",
  "bugs": {
    "url": "https://github.com/mattdesl/browserify-example/issues"
  }
}
